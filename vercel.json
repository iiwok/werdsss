{
  "framework": "nextjs",
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "crons": [
    {
      "path": "/api/cron/random",
      "schedule": "0 0 15 * * *"
    },
    {
      "path": "/api/cron/untranslatable",
      "schedule": "0 5 15 * * *"
    },
    {
      "path": "/api/cron/slang",
      "schedule": "0 9 15 * * *"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@next_public_supabase_url",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_service_role_key"
  }
}
